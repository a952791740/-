    "#生成日历\n",
    "# 引入日历模块\n",
    "import calendar\n",
    "\n",
    "# 输入指定年月\n",
    "yy = int(input(\"输入年份: \"))\n",
    "mm = int(input(\"输入月份: \"))\n",
    "calendar.setfirstweekday(firstweekday=6)#设置第一天是星期天\n",
    "\n",
    "# 显示日历\n",
    "print(calendar.month(yy,mm))"
   ]
